{
	"schemaVersion": 1,
	"id": "autoplayer",
	"version": "${version}",
	"name": "AutoPlayer",
	"description": "Automatically moving the player since 2025!",
	"authors": [
		"Chiefminingdad"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
